on fs
     # Trim partitions
     start emmc_trim

on boot
     # Start LEDify
     start ledify

# eMMC trim
service emmc_trim /system/bin/emmc_trim
     user root
     group shell
     disabled
     oneshot

# LEDify
service ledify /system/bin/ledify
     class late_start
     user root
     group shell
     disabled
     seclabel u:r:init:s0
     oneshot
